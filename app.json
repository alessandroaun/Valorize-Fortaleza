{
  "expo": {
    "name": "Valorize Fortaleza",
    "slug": "ValorizeFortalezaApp",
    "version": "1.0.0",
    "orientation": "portrait",
    
    "icon": "./assets/images/meu_icone.png",
    
    "scheme": "valorizefortalezaapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/meu_icone.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.alessandro.ValorizeFortaleza",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyD46jwK9xQbqae2Gn0Ex3-tNmGmnERmHvA" 
        }
      }
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "3b848994-490a-4302-9eb6-37dec5e98a2e"
      }
    }
  }
}